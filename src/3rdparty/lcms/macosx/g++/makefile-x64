.PHONY: post-build
post-build:
	cp $(OBJ_DIR)/liblcms-pxi.dylib $(PCLBINDIR64)
	codesign --deep -s pleiades -f -v --timestamp $(PCLBINDIR64)/liblcms-pxi.dylib

