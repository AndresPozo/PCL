$(OBJ_DIR)/liblibtiff-pxi.dylib: $(OBJ_FILES)
	clang++ -arch x86_64 -fPIC -headerpad_max_install_names -Wl,-syslibroot,/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX10.10.sdk -mmacosx-version-min=10.7 -stdlib=libc++ -dynamiclib -install_name @executable_path/liblibtiff-pxi.dylib -L"$(PCLLIBDIR64)" -L"$(PCLBINDIR64)" -o $(OBJ_DIR)/liblibtiff-pxi.dylib $(OBJ_FILES) -framework CoreFoundation -ljpeg-pxi -lzlib-pxi
	$(MAKE) -f ./makefile-x64 --no-print-directory post-build

.PHONY: clean
clean:
	rm -f $(OBJ_FILES) $(DEP_FILES) $(OBJ_DIR)/liblibtiff-pxi.dylib

.PHONY: post-build
post-build:
	cp $(OBJ_DIR)/liblibtiff-pxi.dylib $(PCLBINDIR64)
	install_name_tool -change @executable_path/libjpeg-pxi.dylib @loader_path/libjpeg-pxi.dylib $(PCLBINDIR64)/liblibtiff-pxi.dylib
	install_name_tool -change @executable_path/libzlib-pxi.dylib @loader_path/libzlib-pxi.dylib $(PCLBINDIR64)/liblibtiff-pxi.dylib
	codesign --deep -s pleiades -f -v --timestamp $(PCLBINDIR64)/liblibtiff-pxi.dylib

./x64/Release/%.o: ../../%.cxx
	clang++ -c -pipe -arch x86_64 -fPIC -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX10.10.sdk -mmacosx-version-min=10.7 -D_REENTRANT -D__PCL_MACOSX -D"ZIP_SUPPORT" -I"$(PCLINCDIR)" -I"$(PCLSRCDIR)/jpeg" -I"$(PCLSRCDIR)/zlib" -mtune=generic -mssse3 -O3 -ffast-math -ftree-vectorize -std=c++11 -stdlib=libc++ -Wall -Wno-parentheses -Wno-extern-c-compat -MMD -MP -MF"$(@:%.o=%.d)" -o"$@" "$<"
	@echo ' '
