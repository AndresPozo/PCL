$(OBJ_DIR)/libjasper-pxi.dylib: $(OBJ_FILES)
	clang++ -arch x86_64 -Xarch_x86_64 -mmacosx-version-min=10.8 -fPIC -headerpad_max_install_names -dynamiclib -install_name @executable_path/libjasper-pxi.dylib -L"$(PCLLIBDIR64)" -L"$(PCLBINDIR64)" -o $(OBJ_DIR)/libjasper-pxi.dylib $(OBJ_FILES) -framework CoreFoundation
	$(MAKE) -f ./makefile-x64 --no-print-directory post-build

.PHONY: clean
clean:
	rm -f $(OBJ_FILES) $(DEP_FILES) $(OBJ_DIR)/libjasper-pxi.dylib

.PHONY: post-build
post-build:
	cp $(OBJ_DIR)/libjasper-pxi.dylib $(PCLBINDIR64)

