$(OBJ_DIR)/libjasper-pxi.so: $(OBJ_FILES)
	g++ -m64 -fPIC -pthread -Wl,-fuse-ld=gold -Wl,-z,noexecstack -Wl,-O1 -Wl,--gc-sections -s -shared -L"$(PCLLIBDIR64)" -L"$(PCLBINDIR64)" -o $(OBJ_DIR)/libjasper-pxi.so $(OBJ_FILES)
	$(MAKE) -f ./makefile-x64 --no-print-directory post-build

.PHONY: clean
clean:
	rm -f $(OBJ_FILES) $(DEP_FILES) $(OBJ_DIR)/libjasper-pxi.so

.PHONY: post-build
post-build:
	cp $(OBJ_DIR)/libjasper-pxi.so $(PCLBINDIR64)

