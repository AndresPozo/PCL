-include $(DEP_FILES)

$(OBJ_DIR)/installer: $(OBJ_FILES)
	clang++ -m64 -fPIC -Wl,-z,noexecstack -Wl,-O1 -L"$(PCLLIBDIR64)" -L"$(PCLBINDIR64)" -o $(OBJ_DIR)/installer $(OBJ_FILES) -lpthread -lPCL-pxi
