-include $(DEP_FILES)

$(OBJ_DIR)/installer: $(OBJ_FILES)
	g++ -m64 -fPIC -Wl,-fuse-ld=gold -Wl,-z,noexecstack -Wl,-O1 -Wl,--gc-sections -s -L"$(PCLLIBDIR64)" -L"$(PCLBINDIR64)" -o $(OBJ_DIR)/installer $(OBJ_FILES) -lpthread -lPCL-pxi
	$(MAKE) -f ./makefile-x64 --no-print-directory post-build

.PHONY: clean
clean:
	rm -f $(OBJ_FILES) $(DEP_FILES) $(OBJ_DIR)/installer

.PHONY: post-build
post-build:
	cp $(OBJ_DIR)/installer $(PCLBINDIR64)/../..

./x64/Release/%.o: ../../%.cpp
	g++ -c -pipe -m64 -fPIC -D_REENTRANT -D__PCL_LINUX -I"$(PCLINCDIR)" -mtune=corei7 -mfpmath=sse -msse3 -minline-all-stringops -Os -fomit-frame-pointer -ffunction-sections -fdata-sections -fnon-call-exceptions -std=c++11 -Wall -Wno-parentheses -MMD -MP -MF"$(@:%.o=%.d)" -o"$@" "$<"
	@echo ' '

