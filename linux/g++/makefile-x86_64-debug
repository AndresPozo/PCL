libgtest.a:  $(GTEST_OBJ_FILES)
	ar -rv libgtest.a $(GTEST_OBJ_FILES)
	
gtest-all.o : ${GTEST_DIR}/src/gtest-all.cc 
	g++ -isystem ${GTEST_DIR}/include -I${GTEST_DIR} -pthread -c "$<"

gtest_main.o : ${GTEST_DIR}/src/gtest_main.cc
	g++ -isystem ${GTEST_DIR}/include -I${GTEST_DIR} -pthread -c "$<"
	
libgmock.a:  $(GMOCK_OBJ_FILES)
	ar -rv libgmock.a $(GMOCK_OBJ_FILES)
	
gmock-all.o : ${GMOCK_DIR}/src/gmock-all.cc 
	g++ -isystem ${GMOCK_DIR}/include -I${GMOCK_DIR} \
	    -isystem ${GTEST_DIR}/include -I${GTEST_DIR}  -pthread -c "$<"

